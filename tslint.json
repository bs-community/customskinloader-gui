{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-unused-variable": [
			true
		],
		"no-empty": true,
		"no-empty-interface": true,
		"no-reference": true,
		"no-conditional-assignment": true,
		"no-bitwise": true,
		"no-construct": true,
		"no-eval": true,
		"no-for-in-array": true,
		"no-var-keyword": true,
		"no-sparse-arrays": true,
		"no-boolean-literal-compare": true,
		"no-trailing-whitespace": true,
		"no-unnecessary-callback-wrapper": true,
		"only-arrow-functions": [
			true,
			"allow-declarations"
		],
		"switch-default": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"never"
		],
		"triple-equals": true,
		"quotemark": [
			true,
			"single",
			"jsx-double"
		],
		"prefer-for-of": true,
		"await-promise": true,
		"use-isnan": true,
		"typeof-compare": true,
		"restrict-plus-operands": true,
		"eofline": true,
		"space-before-function-paren": [
			true,
			{
				"anonymous": "always",
				"asyncArrow": "always",
				"constructor": "never",
				"method": "always",
				"named": "always"
			}
		],
		"indent": [
			true,
			"spaces",
			2
		],
		"linebreak-style": [
			true,
			"LF"
		],
		"max-line-length": [
			true,
			120
		],
		"prefer-const": true,
		"align": [
			"arguments",
			"parameters",
			"statements"
		],
		"jsdoc-format": true,
		"array-type": [
			"array-simple"
		],
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"arrow-return-shorthand": [
			true,
			"multiline"
		],
		"prefer-method-signature": true,
		"object-literal-key-quotes": [
			true,
			"as-needed"
		],
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-module",
			"check-operator",
			"check-separator",
			"check-type"
		]
	}
}